  "keywords": [
    "wordpress"
  ],
  "support": {
    "issues": "https://github.com/travelsetogreenland/travels/issues"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^8.0",
    "htmlburger/carbon-fields": "^3.3",
    "roots/acorn": "dev-main"
  },
  "require-dev": {
    "filp/whoops": "^2.9",
    "phpunit/phpunit": "8.*",
    "squizlabs/php_codesniffer": "^3.5"
  },
  "suggest": {
    "log1x/sage-directives": "A collection of useful Blade directives for WordPress and Sage (^1.0).",
    "log1x/sage-svg": "A useful SVG directive for inlining SVG's within Blade views (^1.0)."
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "lint": [
      "phpcs --extensions=php --standard=PSR12 app config"
    ],
    "test": "@php ./vendor/phpunit/phpunit/phpunit",
    "post-autoload-dump": [
      "Roots\\Acorn\\ComposerScripts::postAutoloadDump"
    ]
  }
}
